kadmin.local
xst -k hdfs-unmerged.keytab hdfs/hdfs1.david.com@DAVID.COM
xst -k http.keytab HTTP/hdfs1.david.com@DAVID.COM

ktutil
rkt hdfs-unmerged.keytab
rkt http.keytab
wkt hdfs.keytab
clear




